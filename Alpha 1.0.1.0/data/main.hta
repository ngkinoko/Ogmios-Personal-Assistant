</SCRIPT><SCRIPT language="JavaScript" src="database.js"></SCRIPT><SCRIPT language="JavaScript" src="main.js"></SCRIPT><SCRIPT language="JavaScript">
function checkCookie(){
 var enableCookie = getCookie("ogCookiePA");
 
 if(enableCookie == ""){
  Oghide("menu");
  Oghide("Controller");
  Oghide("Versions");
  Ogshow('firstset');
  var h = screen.height;
  var w = screen.width;

 window.resizeTo(w / 2 + 100, h / 2 + 100);
 window.moveTo(w / 4 - 100, h / 4 - 100);


 } else if(enableCookie == "yes"){
  loaded();
 }
}
